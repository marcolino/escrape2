{
  "name": "escrape2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "info": "echo 'escrape2 - the ultimate contacts manager'",
    "clean": "rm -r dist/*",
    "Zstart": "nodemon ./bin/www",
    "Xstart": "npm run lint & npm run test:quiet & node ./bin/www",
    "Xstart:dev": "nodemon --exec 'npm run start'",
    "Xstart:nodev": "nodemon ./bin/www",
    "Ywatch": "echo 'node' & npm run lint & nodemon ./bin/www",
    "Ystart": "npm run lint & npm run test:quiet & node ./bin/www",
    "Ystart-dev": "npm run watch & npm start",
    "Zdebug": "node debug ./bin/www",
    "Zlint": "jshint **/*.js",
    "Zlint:report": "jshint */*.js --reporter checkstyle > .tmp/jshint.checkstyle.xml",
    "Ztest": "mocha --recursive",
    "Ztest:quiet": "mocha --recursive --reporter progress",
    "Zbuild": "# TO BE REWIEWED...",
    "Zprepublish": "npm run build # also runs npm run prebuild",
    "Zprebuild": "npm run test # also runs npm run pretest",
    "Zpretest": "npm run lint",
    "start": "npm run watch-js",
    "watch-js": "nodemon -e js -w . -x 'npm run test'",
    "Awatch-sass": "nodemon -e scss -w public/sass -x 'npm run build-sass'",
    "test": "npm run lint & npm run mocha:quiet",
    "jscs": "jscs **/*.js",
    "lint": "jshint **/*.js",
    "lint:report": "jshint */*.js --reporter checkstyle > .tmp/jshint.checkstyle.xml",
    "mocha": "mocha --recursive",
    "mocha:quiet": "mocha --recursive --reporter progress"
  },
  "dependencies": {
    "async": "^1.4.2",
    "body-parser": "^1.13.3",
    "chai": "^3.3.0",
    "cheerio": "^0.19.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "delayed-request": "0.0.3",
    "express": "~4.13.1",
    "function-rate-limit": "^1.0.1",
    "imagediff": "^1.0.8",
    "jade": "~1.11.0",
    "md5": "^2.0.0",
    "method-override": "^2.3.5",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.1.6",
    "morgan": "~1.6.1",
    "random-useragent": "^0.1.1",
    "request": "^2.62.0",
    "requestretry": "^1.5.0",
    "serve-favicon": "~2.3.0",
    "socks5-http-client": "^1.0.1"
  },
  "devDependencies": {
    "jshint": "latest",
    "mocha": "^2.3.3",
    "sinon": "^1.17.1",
    "superagent": "^1.4.0",
    "jscs": "~2.2.1"
  },
  "jshintConfig": {
    "laxcomma": true
  }
}
