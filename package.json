{
  "name": "escrape2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "info": "echo 'escrape2 - the ultimate contacts manager'",
    "clean": "rm -r dist/*",
    "start": "nodemon ./bin/www",
    "Xstart": "npm run lint & npm run test:quiet & node ./bin/www",
    "Xstart:dev": "nodemon --exec 'npm run start'",
    "Xstart:nodev": "nodemon ./bin/www",
    "Ywatch": "echo 'node' & npm run lint & nodemon ./bin/www",
    "Ystart": "npm run lint & npm run test:quiet & node ./bin/www",
    "Ystart-dev": "npm run watch & npm start",
    "debug": "node debug ./bin/www",
    "lint": "jshint **/*.js",
    "lint:report": "jshint */*.js --reporter checkstyle > .tmp/jshint.checkstyle.xml",
    "test": "mocha --recursive",
    "test:quiet": "mocha --recursive --reporter progress",
    "build": "# TO BE REVIRWED...",
    "prepublish": "npm run build # also runs npm run prebuild",
    "prebuild": "npm run test # also runs npm run pretest",
    "pretest": "npm run lint"
  },
  "dependencies": {
    "async": "^1.4.2",
    "body-parser": "^1.13.3",
    "chai": "^3.3.0",
    "cheerio": "^0.19.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "delayed-request": "0.0.3",
    "express": "~4.13.1",
    "function-rate-limit": "^1.0.1",
    "imagediff": "^1.0.8",
    "jade": "~1.11.0",
    "md5": "^2.0.0",
    "method-override": "^2.3.5",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.1.6",
    "morgan": "~1.6.1",
    "random-useragent": "^0.1.1",
    "request": "^2.62.0",
    "requestretry": "^1.5.0",
    "serve-favicon": "~2.3.0",
    "socks5-http-client": "^1.0.1"
  },
  "devDependencies": {
    "jshint": "latest",
    "mocha": "^2.3.3",
    "sinon": "^1.17.1",
    "superagent": "^1.4.0"
  },
  "jshintConfig": {
    "laxcomma": true
  }
}
