{
  "name": "escrape2",
  "description": "The ultimate contacts manager",
  "private": true,
  "version": "0.0.0",
  "repository": "https://github.com/marcolino/escrape2",
  "license": "MIT",
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install --silent",
    "start": "nodemon ./bin/www",
    "restart-full-tests": "echo 'restart with lint, jscs mocha' & npm run test",
    "test": "npm run lint & npm run jscs & npm run mocha",
    "jscs": "jscs **/*.js",
    "lint": "jshint **/*.js",
    "lint:report": "jshint */*.js --reporter checkstyle > .tmp/jshint.checkstyle.xml",
    "mocha": "mocha --recursive",
    "mocha:quiet": "mocha --recursive --reporter progress",
    "info": "echo 'escrape2 - the ultimate contacts manager'",
    "clean": "rm -r dist/*"
  },
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.13.3",
    "chai": "^3.3.0",
    "cheerio": "^0.19.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "delayed-request": "0.0.3",
    "express": "~4.13.1",
    "function-rate-limit": "^1.0.1",
    "hamming-distance": "^1.0.0",
    "imagediff": "^1.0.8",
    "jade": "~1.11.0",
    "lodash": "^3.10.1",
    "md5": "^2.0.0",
    "method-override": "^2.3.5",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.1.6",
    "morgan": "~1.6.1",
    "ng-annotate": "^1.0.2",
    "phash-image": "^3.3.0",
    "request": "^2.62.0",
    "requestretry": "^1.5.0",
    "serve-favicon": "~2.3.0",
    "simple-node-logger": "^0.92.21",
    "socket.io": "^1.3.7",
    "socks5-http-client": "^1.0.1"
  },
  "devDependencies": {
    "bower": "^1.6.3",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-concurrent": "^2.0.3",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-uglify": "^0.9.2",
    "grunt-usemin": "^3.1.1",
    "grunt-wiredep": "^2.0.0",
    "jscs": "~2.2.1",
    "jshint": "latest",
    "load-grunt-tasks": "^3.3.0",
    "mocha": "^2.3.3",
    "random-ua": "0.0.6",
    "shelljs": "^0.2.6",
    "sinon": "^1.17.1",
    "superagent": "^1.4.0",
    "time-grunt": "^1.2.2"
  },
  "jshintConfig": {
    "laxcomma": true
  }
}
